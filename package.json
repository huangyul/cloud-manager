    "@vitejs/plugin-vue": "^4.0.0",
    "cli": "link:.git\\@commitlint\\cli",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.4",
    "sass": "^1.59.3",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vue-tsc": "^1.0.24"
  },
  "lint-staged": {
    "*.{js,ts,vue,jsx,tsx}": [
      "prettier --write"
    ]
